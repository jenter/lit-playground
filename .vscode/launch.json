{
  "version": "0.1.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "WCAanalyzer",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "wca-analyze"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "mrTypescript",
      "program": "${workspaceFolder}/src/test.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true,
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
    // {
    //   "type": "node",
    //   "request": "launch",
    //   "name": "DebugXXXBbuild",
    //   "runtimeExecutable": "npm",
    //   "runtimeArgs": ["run-script", "build"],
    //   "console": "integratedTerminal",
    //   "skipFiles": ["<node_internals>/**"],
    //   "sourceMaps": true
    // }
  ]
}
